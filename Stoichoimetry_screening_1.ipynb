        "# Step 2: Visualize Binary Stoichiometric Validity Grid (Na–Fe example)\n",
        "def plot_stoichiometric_grid(df, element_A, element_B, max_display=5):\n",
        "    \"\"\"\n",
        "    Plot a 2D grid showing SMACT-valid stoichiometries for a binary system.\n",
        "\n",
        "    Args:\n",
        "        df (DataFrame): Output from screen_binary_stoichiometries()\n",
        "        element_A, element_B (str): Element symbols (e.g. \"Na\", \"Fe\")\n",
        "        max_display (int): Max coefficient to show on axes\n",
        "    \"\"\"\n",
        "    grid = np.zeros((max_display, max_display))\n",
        "\n",
        "    for _, row in df.iterrows():\n",
        "        a = row['coeff_A']\n",
        "        b = row['coeff_B']\n",
        "        if a <= max_display and b <= max_display:\n",
        "            grid[a-1, b-1] = 1 if row['valid'] else -1\n",
        "\n",
        "    fig, ax = plt.subplots(figsize=(8, 6))\n",
        "    im = ax.imshow(\n",
        "        grid, cmap='RdYlGn', aspect='equal', origin='lower',\n",
        "        extent=[0.5, max_display+0.5, 0.5, max_display+0.5]\n",
        "    )\n",
        "\n",
        "    for i in range(max_display):\n",
        "        for j in range(max_display):\n",
        "            val = grid[i, j]\n",
        "            label = \"Valid\" if val == 1 else \"Invalid\" if val == -1 else \"\"\n",
        "            color = \"black\" if val != 0 else \"grey\"\n",
        "            ax.text(j+1, i+1, label, ha='center', va='center',\n",
        "                    color=color, fontsize=10, weight='bold')\n",
        "\n",
        "    ax.set_xlabel(f\"{element_B} stoichiometric coefficient\", fontsize=12)\n",
        "    ax.set_ylabel(f\"{element_A} stoichiometric coefficient\", fontsize=12)\n",
        "    ax.set_title(f\"Stoichiometric Validity Grid: {element_A}–{element_B}\", fontsize=14)\n",
        "    ax.set_xticks(range(1, max_display+1))\n",
        "    ax.set_yticks(range(1, max_display+1))\n",
        "\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
