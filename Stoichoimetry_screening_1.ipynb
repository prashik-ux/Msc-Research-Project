        "def screen_binary_stoichiometries(element_A, element_B, max_stoich=5):\n",
        "    \"\"\"\n",
        "    Screen all possible stoichiometries for a binary system AₓBᵧ.\n",
        "\n",
        "    Args:\n",
        "        element_A, element_B (str): Element symbols\n",
        "        max_stoich (int): Maximum stoichiometric coefficient\n",
        "\n",
        "    Returns:\n",
        "        pandas.DataFrame: Results with compositions and validity\n",
        "    \"\"\"\n",
        "    results = []\n",
        "\n",
        "    for x in range(1, max_stoich + 1):\n",
        "        for y in range(1, max_stoich + 1):\n",
        "            try:\n",
        "                comp = Composition({element_A: x, element_B: y}).reduced_composition\n",
        "                is_valid = smact_validity(comp)\n",
        "                results.append({\n",
        "                    'stoichiometry': f\"{element_A}{x}{element_B}{y}\",\n",
        "                    'composition': comp,\n",
        "                    'formula': comp.reduced_formula,\n",
        "                    'valid': is_valid,\n",
        "                    'coeff_A': x,\n",
        "                    'coeff_B': y\n",
        "                })\n",
        "            except Exception as e:\n",
        "                # Handle any parsing issues gracefully\n",
        "                results.append({\n",
        "                    'stoichiometry': f\"{element_A}{x}{element_B}{y}\",\n",
        "                    'composition': None,\n",
        "                    'formula': None,\n",
        "                    'valid': False,\n",
        "                    'coeff_A': x,\n",
        "                    'coeff_B': y\n",
        "                })\n",
        "\n",
        "    df = pd.DataFrame(results)\n",
        "    df = df.drop_duplicates('formula').reset_index(drop=True)\n",
        "    return df\n",
        "\n",
        "#  Screen binary systems for Na-Fe-O\n",
        "binary_pairs = [(\"Na\", \"Fe\"), (\"Na\", \"O\"), (\"Fe\", \"O\")]\n",
        "binary_results = {}\n",
        "\n",
        "for elA, elB in binary_pairs:\n",
        "    print(f\"\\n Screening binary system: {elA}-{elB}\")\n",
        "    df_result = screen_binary_stoichiometries(elA, elB, max_stoich=5)\n",
        "    binary_results[f\"{elA}-{elB}\"] = df_result\n",
        "\n",
        "    # Display summary\n",
        "    valid_count = df_result['valid'].sum()\n",
        "    total_count = len(df_result)\n",
        "    print(f\"   Found {len(df_result)} unique stoichiometries\")\n",
        "    print(f\"   Valid stoichiometries: {valid_count}/{total_count} ({valid_count/total_count*100:.1f}%)\")\n",
        "    print(f\"   Examples:\\n{df_result[['formula', 'valid']].head(6)}\")\n"
      ],
